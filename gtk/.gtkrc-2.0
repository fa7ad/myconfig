# Configs for GTK2 programs

include "/home/fahad/.themes/wal/gtk-2.0/gtkrc"
style "user-font"
{
  font_name="Google Sans Medium 10"
}
widget_class "*" style "user-font"
gtk-font-name="Google Sans Medium 10"
gtk-theme-name="wal"
gtk-icon-theme-name="Papirus"
gtk-fallback-icon-theme="Numix"
gtk-cursor-theme-name="Numix-Cursor"
gtk-toolbar-style=GTK_TOOLBAR_ICONS
gtk-menu-images=1
gtk-button-images=1
gtk-primary-button-warps-slider=0
