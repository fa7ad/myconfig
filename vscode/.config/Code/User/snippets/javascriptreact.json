{
  // Possible variables are:
  // $1, $2 for tab stops,
  // ${id} and ${id:label} and ${1:label} for variables.
  // Variables with the same id are connected.
  // Example:
  "ConsoleLog": {
    "prefix": "log",
    "body": [
      "console.log('$1')"
    ],
    "description": "Log output to console"
  },
  "ImportES2015": {
    "prefix": "impes",
    "body": "import ${2:variable} from '${1:component}'",
    "description": "Import the default export using es2015 syntax"
  },
  "ExportES2015": {
    "prefix": "expes",
    "body": "export default ${1:variable}",
    "description": "Export a variable as default export using es2015 syntax"
  },
  "CommonJSRequire": {
    "prefix": "req",
    "body": "const ${1:variable} = require('${2:module}')$3",
    "description": "Require a commonjs module"
  },
  "ImportReactES2015": {
    "prefix": "rees",
    "body": [
      "import React, {Component} from 'react'",
      "$1"
    ],
    "description": "Import react using es2015 syntax"
  },
  "ImportReactComponentES2015": {
    "prefix": "rces",
    "body": [
      "import { Component } from 'react'",
      "$1"
    ],
    "description": "Import React.Component using es2015 syntax"
  },
  "ReactES2015Component": {
    "prefix": "recom",
    "body": [
      "class $1 extends Component {",
      "  render () {",
      "    return (",
      "      $2",
      "    )",
      "  }",
      "}",
      "",
      "export default $1",
      ""
    ],
    "description": "Create a react component"
  },
  "ReactFuncionalComponent": {
    "prefix": "rfcom",
    "body": [
      "const $1 = (props) => (",
      "  $2",
      ")",
      "",
      "export default $1",
      ""
    ],
    "description": "Create a functional react component"
  },
  "ImportMobxReact": {
    "prefix": "immx",
    "body": "import {$1} from 'mobx-react'$2",
    "description": "Import mobx-react utilities"
  },
  "ImportReactPropType": {
    "prefix": "repr",
    "body": [
      "import PropTypes from 'prop-types'$1"
    ],
    "description": "Import react prop-types"
  },
  "ReactES7PropTypes": {
    "prefix": "prtyc",
    "body": [
      "static propTypes = {",
      "  $1",
      "}"
    ],
    "description": "Define react proptypes using es7 class properties"
  },
  "ReactFunctionalPropTypes": {
    "prefix": "prtyf",
    "body": [
      "$1.propTypes = {",
      "  $2",
      "}"
    ],
    "description": "Define react proptypes for functional component"
  },
  "FixLazy": {
    "prefix": "fxlz",
    "body": [
      "import React from 'react'",
      "import PropTypes from 'prop-types'",
      "import styled from 'styled-components'"
    ]
  }
}